{
  "name": "ember-c3",
  "version": "3.0.2",
  "description": "Ember addon library for C3, a D3-based reusable chart library.",
  "keywords": [
    "ember-addon",
    "ember-cli",
    "ember-c3",
    "c3",
    "d3",
    "chart",
    "graph"
  ],
  "repository": "https://github.com/Glavin001/ember-c3",
  "license": "MIT",
  "author": {
    "name": "Glavin Wiechert",
    "email": "glavin.wiechert@gmail.com",
    "url": "https://github.com/Glavin001"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "contributors": [
    "maxwondercorn <> (https://github.com/maxwondercorn)"
  ],
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "demoURL": "http://glavin001.github.io/ember-c3/",
    "configPath": "tests/dummy/config"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "auto-changelog -u --output CHANGELOG.md --commit-limit false",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "deploy": "ember build && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "unreleased": true,
    "commitLimit": false
  },
  "dependencies": {
    "broccoli-funnel": "^2.0.2",
    "broccoli-merge-trees": "^3.0.2",
    "c3": "^0.7.20",
    "d3": "^6.6.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli-babel": "^7.17.2",
    "ember-cli-htmlbars": "^4.2.2"
  },
  "devDependencies": {
    "@ember/jquery": "^1.1.0",
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "auto-changelog": "^1.16.2",
    "babel-eslint": "^10.0.3",
    "bootstrap": "^4.3.1",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-classic-decorator": "^1.0.3",
    "ember-cli": "~3.16.2",
    "ember-cli-bootstrap-4": "^0.11.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-qunit": "^4.4.0",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-code-snippet": "^2.4.1",
    "ember-decorators": "^6.1.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-font-awesome": "^4.0.0-rc.4",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.16.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint-plugin-ember": "^7.7.2",
    "eslint-plugin-node": "^11.0.0",
    "loader.js": "^4.7.0",
    "popper.js": "^1.16.0",
    "qunit-dom": "^1.6.0",
    "sass": "^1.23.7"
  },
  "engines": {
    "node": "10.* || >= 12"
  }
}
